@model IEnumerable<SchoolPersonelsManagement.Models.Lesson>
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="row align-items-center ml-4 mb-2 justify-content-between" style="width: 40%; height: 5rem;
                    border: 1px solid; border-radius: 10px; box-shadow: 5px 10px 18px #888888;">
                <h3 class="ml-1">@item.LessonName</h3>
                <a class="btn btn-outline-success mr-1">Bu dersi verebilecek Öğretmenler</a>
            </div>

        }
    </div>